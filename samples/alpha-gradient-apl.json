{
  "type": "APL",
  "version": "1.9",
  "theme": "light",
  "mainTemplate": {
    "parameters": [
      "payload"
    ],
    "items": {
      "type": "VectorGraphic",
      "source": "myGraphic",
      "width": "100%",
      "height": "100%",
      "scale": "best-fit",
      "align": "center"
    }
  },
  "graphics": {
    "myGraphic": {
      "type": "AVG",
      "version": "1.2",
      "width": 960,
      "height": 600,
      "items": {
        "type": "group",
        "bind": [
          {
            "name": "frame",
            "value": "${(elapsedTime*0.06)%301}"
          }
        ],
        "items": [
          {
            "items": [
              {
                "type": "path",
                "pathData": "M480,-300 l0,600 l-960,0 l0,-600 z",
                "fill": "rgb(255,0,0,1)"
              }
            ],
            "type": "group",
            "translateX": 480,
            "translateY": 300
          },
          {
            "items": [
              {
                "type": "path",
                "pathData": "M0,-300 a300,300 0 0 1  0,600 a300,300 0 0 1  0,-600",
                "fill": "@gradient1000",
                "fillTransform": "translate(0.5, 0.5) scale(1,1) translate(-0.5,-0.5)"
              }
            ],
            "type": "group",
            "translateX": 480,
            "translateY": 300,
            "rotation": "${@ease1(frame)}"
          }
        ]
      },
      "resources": [
        {
          "easing": {
            "ease1": "curve(0,0,0.167,0.167,0.833,0.833) end(300,360)"
          }
        },
        {
          "gradient": {
            "gradient1000": {
              "type": "linear",
              "inputRange": [
                0,
                0.5,
                1
              ],
              "colorRange": [
                "rgba(0,0,0,0)",
                "rgba(0,0,0,0.5)",
                "rgba(0,0,0,1)"
              ],
              "x1": 0.003196666666666677,
              "x2": 0.998815,
              "y1": 0.5,
              "y2": 0.5
            }
          }
        }
      ]
    }
  }
}